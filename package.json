{
  "name": "music-player",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "js": "browserify -d src/js/index.js -o src/js/main.js",
    "js-old": "browserify -d src/js/dev/main.js -o src/js/main.js",
    "scss": "node-sass src/scss/main.scss --source-map-embed src/css/main.css && npm run autoprefixer",
    "watch:scss": "onchange 'src/scss/*.scss' 'src/scss/imports/*.scss' -- npm run scss",
    "watch:js": "onchange 'src/js/index.js' 'src/js/dev/*.js' 'src/js/dev/**/*.js' -- npm run js",
    "watch": "npm run watch:js & npm run watch:scss",
    "start": "live-server & npm run watch",
    "autoprefixer": "postcss -u autoprefixer src/css/main.css -d src/css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "soundcloud": "^3.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.0.1",
    "eslint": "^2.12.0",
    "node-sass": "^3.7.0",
    "onchange": "^2.5.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
